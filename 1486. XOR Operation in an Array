class Solution {
    public int xorOperation(int n, int start) {
        //Create array to store value of start+2*i
        int[] arr=new int[n];
        int result=0;
        for(int i=0;i<n;i++){
            arr[i]=start+2*i;
            //Perform XOR operation to every value of array and return result
            result^=arr[i];
        }
        return result;
    }
}
